{
  "dashboard": {
    "title": "CEA Temperature Alerts",
    "tags": ["cea", "temperature", "alerts"],
    "timezone": "browser"
  },
  "panels": [
    {
      "id": 1,
      "title": "Dry Bulb Temperature High - Back",
      "type": "graph",
      "targets": [
        {
          "refId": "A",
          "rawSql": "SELECT time, value FROM measurement_with_metadata WHERE sensor_name = 'dry_bulb_b' AND time > $__timeFrom() AND time < $__timeTo() ORDER BY time DESC LIMIT 1",
          "format": "time_series",
          "datasource": "CEA Sensors"
        }
      ],
      "alert": {
        "name": "Dry Bulb Temperature High - Back",
        "message": "Dry bulb temperature (back) exceeded maximum threshold of 35°C",
        "conditions": [
          {
            "evaluator": {
              "params": [35],
              "type": "gt"
            },
            "operator": {
              "type": "and"
            },
            "query": {
              "params": ["A", "5m", "now"]
            },
            "reducer": {
              "params": [],
              "type": "last"
            },
            "type": "query"
          }
        ],
        "executionErrorState": "alerting",
        "for": "1m",
        "frequency": "30s",
        "handler": 1,
        "noDataState": "no_data",
        "notifications": []
      }
    },
